#include<bits/stdc++.h>

int max(int j,int k){
	if(j>=k) return j;
	else return k;
}

int main(){
	int n,m,j,k;
	int pa[1000]={0},pb[1000]={0},ans[1000]={0};
	scanf("%d %d",&n,&m);
	for(int i=0;i<n;i++){
		scanf("%d",&j);
		scanf("%d",&pa[j]);
	}
	for(int i=0;i<m;i++){
		scanf("%d",&k);
		scanf("%d",&pb[k]);
	}
	
	for(int i=0;i<max(j,k)+1;i++){
		ans[i]=pa[i]+pb[i];
		if(ans[i]!=0){
			printf("%d %d\n",i,ans[i]);
		}
	}
	
	return 0;
}
